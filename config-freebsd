show_cmd=0
show_conf=0
upsd_path=/usr/local/sbin/upsd
ups_name=ups
nut_confpath=/usr/local/etc/nut
upsschedcmd_path=/usr/local/bin/upssched-cmd
nut_monitor=battery.*:|device.*:|driver.*:|input.*:|ups.*:
